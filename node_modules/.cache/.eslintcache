Id);\n\t\t\t\tconsole.log('action.payload.quantity');\n\t\t\t\tconsole.log(action.payload.quantity);\n\t\t\t\tdraft.cart_list[idx] = { ...draft.cart_list[idx], quantity: action.payload.quantity };\n\t\t\t}),\n\t},\n\tinitialState,\n);\n\nconst actionCreators = {\n\taddCardMiddleWare,\n\tgetCartAPI,\n\tdeleteCartMddleWares,\n\tupdateQuantity,\n\tupdateCart,\n};\n\nexport { actionCreators };","/Users/jeonghwan/Desktop/Clonemk/clone_mk/src/redux/modules/product.js",[],[],"/Users/jeonghwan/Desktop/Clonemk/clone_mk/src/shared/api.js",[],[],"/Users/jeonghwan/Desktop/Clonemk/clone_mk/src/elements/index.js",[],[],"/Users/jeonghwan/Desktop/Clonemk/clone_mk/src/elements/Text.js",[],[],"/Users/jeonghwan/Desktop/Clonemk/clone_mk/src/elements/Grid.js",[],[],"/Users/jeonghwan/Desktop/Clonemk/clone_mk/src/elements/Input.js",[],[],"/Users/jeonghwan/Desktop/Clonemk/clone_mk/src/elements/Button.js",[],[],"/Users/jeonghwan/Desktop/Clonemk/clone_mk/src/components/Header.js",["352","353"],[],"// Header.js\n\n// import를 한다.\nimport React, { useEffect } from 'react';\nimport styled from 'styled-components';\nimport { Grid } from '../elements/index';\nimport { history } from '../redux/configureStore';\nimport { useSelector, useDispatch } from 'react-redux';\nimport { actionCreators as userActions } from '../redux/modules/user';\n\n// image\nimport HeaderLeftImg from '../images/header-left-delivery.gif';\n\n// Header의 함수형 컴포넌트를 만든다.\nconst Header = props => {\n\tconst dispatch = useDispatch();\n\tconst is_login = useSelector(state => state.user.is_login);\n\tconst userInfo = useSelector(state => state.user.user);\n\n\tconst headerChange = () => {\n\t\tconst headerbox = document.querySelector('.header');\n\t\tconst loginbox = document.querySelector('.scroll-event');\n\n\t\tif (window.scrollY > 105) {\n\t\t\tloginbox.style.display = 'none';\n\t\t\theaderbox.style.position = 'fixed';\n\t\t\theaderbox.style.zIndex = '300';\n\t\t\theaderbox.style.width = '1050px';\n\t\t} else {\n\t\t\tloginbox.s